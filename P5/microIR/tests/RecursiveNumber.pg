MAIN
  PRINT CALL
    BEGIN
      MOVE TEMP 20
        BEGIN
          MOVE TEMP 23 HALLOCATE 4
          MOVE TEMP 24 HALLOCATE 4
          HSTORE TEMP 23 0 Test_fun
          HSTORE TEMP 24 0 TEMP 23
        RETURN TEMP 24
        END
      HLOAD TEMP 21 TEMP 20 0
      HLOAD TEMP 22 TEMP 21 0
    RETURN TEMP 22
    END ( TEMP 20 10 )
END

Test_fun [ 2 ]
  BEGIN
    CJUMP LT 1 TEMP 1 L0
    PRINT TEMP 1
    PRINT CALL
      BEGIN
        MOVE TEMP 25 TEMP 0
        HLOAD TEMP 26 TEMP 25 0
        HLOAD TEMP 27 TEMP 26 0
      RETURN TEMP 27
      END ( TEMP 25 MINUS TEMP 1 1 )
    JUMP L1
    L0 NOOP
    MOVE TEMP 1 TEMP 1
    L1 NOOP
  RETURN TEMP 1
  END
